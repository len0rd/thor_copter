<!--Basic launch file to get thor in the air with sensors connected to ros
    @author @len0rd
    @since 2018-10-11 -->

<launch>

    <!-- Start rosflight -->
    <node pkg="rosflight" type="rosflight_io" name="rosflight" output="screen">
        <param name="port" value="/dev/naze"/>
        <param name="baud" value="921600"/>
    </node>

</launch>
